{
  "name": "purchase-tracker-backend",
  "version": "1.0.0",
  "description": "Backend service for Purchase Tracker app with AWS Cognito, S3, and Textract integration",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "purchase-tracker",
    "nodejs",
    "express",
    "aws-cognito",
    "textract",
    "s3"
  ],
  "author": "Purchase Tracker Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-textract": "^3.400.0",
    "@aws-sdk/s3-request-presigner": "^3.400.0",
    "aws-jwt-verify": "^4.0.1",
    "prisma": "^5.2.0",
    "@prisma/client": "^5.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
